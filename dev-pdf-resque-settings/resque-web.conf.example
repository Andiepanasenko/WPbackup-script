[program:resque-web-DEV_NUM]
directory=/opt/node-resque/resque-web-DEV_NUM/
command=bundle exec rackup -p 39DEV_NUM
numprocs=1
user=root
autostart=true
autorestart=true
stopsignal=TERM
;stopasgroup=true
stopwaitsecs=1
process_name = %(process_num)s
redirect_stderr=true
stdout_logfile=/var/log/supervisor/resque_webDEV_NUM.log
stdout_logfile_maxbytes=1MB
stdout_logfile_backups=5
stdout_capture_maxbytes=1MB
stderr_logfile=/var/log/supervisor/resque_webDEV_NUM.log
stderr_logfile_maxbytes=1MB
stderr_logfile_backups=10
stderr_capture_maxbytes=1MB

